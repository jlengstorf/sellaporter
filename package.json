{
  "name": "sellaporter",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production npm run server",
    "develop": "NODE_ENV=development concurrent --kill-others 'npm run watch:assets' 'npm run server' 'npm run livereload'",
    "deploy": "BUILD_ENV=production NODE_ENV=production npm run build",
    "build:assets": "webpack --progress --colors --optimize-minimize --optimize-dedupe",
    "build:site": "punch g",
    "server": "punch s",
    "livereload": "live-reload --port 9091 templates/",
    "watch:styles": "watch 'npm run build:assets' styles/",
    "watch:scripts": "watch 'npm run build:assets' scripts/",
    "watch:assets": "npm run watch:styles && npm run watch:scripts",
    "watch:templates": "watch 'npm run build:site' templates/",
    "test": "echo 'Error: no test specified' && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^1.0.0",
    "getconfig": "^2.2.0",
    "jade": "^1.11.0",
    "punch": "^0.5.46",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "css-loader": "^0.23.1",
    "cssnano": "^3.4.0",
    "cssnext": "^1.8.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "live-reload": "^1.1.0",
    "postcss-import": "^7.1.3",
    "postcss-loader": "^0.8.0",
    "postcss-mixins": "^3.0.2",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^1.2.0",
    "style-loader": "^0.13.0",
    "watch": "^0.17.1",
    "webpack": "^1.12.10",
    "webpack-dev-server": "^1.14.0"
  }
}
